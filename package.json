  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexZeDim/conglomerat.git"
  },
  "languageName": "node",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/conglomerat/test/jest-e2e.json",
    "docker:build": "docker build -f local.Dockerfile -t cmnw:%npm_package_version% -t cmnw:latest .",
    "version": "echo %npm_package_version%"
  },
  "dependencies": {
    "@anchan828/nest-bullmq": "3.1.30",
    "@discordjs/builders": "^1.6.3",
    "@discordjs/voice": "0.16.0",
    "@nestjs-modules/ioredis": "1.0.1",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.0.2",
    "@nestjs/core": "10.0.2",
    "@nestjs/mongoose": "10.0.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.2",
    "@nestjs/schedule": "3.0.1",
    "@nestjs/swagger": "7.0.2",
    "@nestjs/typeorm": "10.0.0",
    "@types/async-csv": "2.1.1",
    "@types/cheerio": "0.22.31",
    "@types/crypto-js": "4.1.1",
    "@types/fs-extra": "11.0.1",
    "async-csv": "2.1.3",
    "blizzapi": "2.6.0",
    "bull-board": "2.1.3",
    "bullmq": "3.15.6",
    "cheerio": "1.0.0-rc.12",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "config": "3.3.4",
    "crypto-js": "4.1.1",
    "discord-api-types": "0.37.46",
    "discord.js": "14.11.0",
    "farmhash": "3.3.0",
    "fs-extra": "11.1.1",
    "ioredis": "5.3.2",
    "lodash": "4.17.21",
    "mongoose": "7.3.0",
    "ms": "2.1.3",
    "passport": "0.6.0",
    "passport-oauth2": "1.7.0",
    "pg": "^8.11.0",
    "playwright": "1.36.0-alpha-jul-2-2023",
    "redis": "4.6.7",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1",
    "snake-case": "3.0.4",
    "swagger-ui-express": "4.6.3",
    "typeorm": "0.3.16",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@discordjs/rest": "1.7.1",
    "@nestjs/cli": "10.0.3",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.2",
    "@types/config": "3.3.0",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.17",
    "@types/ioredis": "5.0.0",
    "@types/jest": "29.5.2",
    "@types/lodash": "4.14.195",
    "@types/ms": "0.7.31",
    "@types/multer": "1.4.7",
    "@types/node": "20.3.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.43.0",
    "eslint-config-airbnb-typescript": "17.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "29.5.0",
    "nanoid": "4.0.2",
    "prettier": "2.8.8",
    "run-script-webpack-plugin": "^0.2.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "webpack": "^5.87.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/configuration/(.*)": "<rootDir>/libs/configuration/src/$1",
      "@app/configuration": "<rootDir>/libs/configuration/src",
      "@app/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@app/core": "<rootDir>/libs/core/src",
      "@app/mongo/(.*)": "<rootDir>/libs/mongo/src/$1",
      "@app/mongo": "<rootDir>/libs/mongo/src",
      "^@app/elastic(|/.*)$": "<rootDir>/libs/elastic/src/$1",
      "^@app/e2e(|/.*)$": "<rootDir>/libs/e2e/src/$1",
      "^@app/pg(|/.*)$": "<rootDir>/libs/pg/src/$1"
    }
  }
}
