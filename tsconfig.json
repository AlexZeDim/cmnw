{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "paths": {
      "@app/configuration": [
        "libs/configuration/src"
      ],
      "@app/configuration/*": [
        "libs/configuration/src/*"
      ],
      "@app/core": [
        "libs/core/src"
      ],
      "@app/core/*": [
        "libs/core/src/*"
      ],
      "@app/character-queue-osint": [
        "libs/character-queue-osint/src"
      ],
      "@app/character-queue-osint/*": [
        "libs/character-queue-osint/src/*"
      ],
      "@app/realm-queue-osint": [
        "libs/realm-queue-osint/src"
      ],
      "@app/realm-queue-osint/*": [
        "libs/realm-queue-osint/src/*"
      ],
      "dma/item-queue": [
        "libs/item-queue/src"
      ],
      "dma/item-queue/*": [
        "libs/item-queue/src/*"
      ],
      "@app/mongo": [
        "libs/mongo/src"
      ],
      "@app/mongo/*": [
        "libs/mongo/src/*"
      ]
    }
  }
}