    "mongoose": "7.2.1",
    "ms": "2.1.3",
    "passport": "0.6.0",
    "passport-oauth2": "1.6.1",
    "prism-media": "2.0.0-alpha.0",
    "puppeteer": "13.0.0",
    "redis": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "swagger-ui-express": "4.3.0"
  },
  "devDependencies": {
    "@discordjs/rest": "0.1.0-canary.0",
    "@nestjs/cli": "8.1.6",
    "@nestjs/schematics": "8.0.5",
    "@nestjs/testing": "8.2.4",
    "@types/config": "0.0.39",
    "@types/cron": "1.7.3",
    "@types/express": "4.17.13",
    "@types/ioredis": "4.28.5",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.178",
    "@types/ms": "0.7.31",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.0",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "eslint": "8.41.0",
    "eslint-config-airbnb-typescript": "17.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "27.4.5",
    "nanoid": "3.2.0",
    "prettier": "2.5.1",
    "supertest": "6.1.6",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.3.5",
    "webpack": "5.58.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/configuration/(.*)": "<rootDir>/libs/configuration/src/$1",
      "@app/configuration": "<rootDir>/libs/configuration/src",
      "@app/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@app/core": "<rootDir>/libs/core/src",
      "@app/mongo/(.*)": "<rootDir>/libs/mongo/src/$1",
      "@app/mongo": "<rootDir>/libs/mongo/src",
      "^@app/elastic(|/.*)$": "<rootDir>/libs/elastic/src/$1",
      "^@app/e2e(|/.*)$": "<rootDir>/libs/e2e/src/$1"
    }
  }
}
