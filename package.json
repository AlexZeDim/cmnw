  "languageName": "node",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/conglomerat/test/jest-e2e.json",
    "docker:build": "docker build -f local.Dockerfile -t cmnw:%npm_package_version% -t cmnw:latest .",
    "version": "echo %npm_package_version%"
  },
  "dependencies": {
    "@anchan828/nest-bullmq": "1.0.23",
    "@discordjs/voice": "0.7.5",
    "@elastic/elasticsearch": "7.16.0",
    "@nestjs-modules/ioredis": "1.0.1",
    "@nestjs/axios": "0.0.3",
    "@nestjs/common": "9.0.5",
    "@nestjs/core": "9.0.5",
    "@nestjs/mongoose": "9.0.1",
    "@nestjs/passport": "8.2.2",
    "@nestjs/platform-express": "8.2.4",
    "@nestjs/schedule": "1.0.2",
    "@nestjs/swagger": "5.1.5",
    "@nlpjs/core": "4.22.0",
    "@nlpjs/emoji": "4.22.0",
    "@types/cheerio": "0.22.30",
    "@types/crypto-js": "4.0.2",
    "@types/fs-extra": "9.0.13",
    "async-csv": "2.1.3",
    "blizzapi": "2.0.4",
    "bull-board": "2.1.3",
    "bullmq": "1.59.2",
    "cheerio": "1.0.0-rc.10",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "config": "3.3.6",
    "crypto-js": "4.1.1",
    "discord-api-types": "0.25.2",
    "discord.js": "13.3.1",
