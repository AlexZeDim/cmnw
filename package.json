{
  "name": "cmnw",
  "version": "6.1.30",
  "homepage": "https://cmnw.me",
  "description": "Intelligence always wins",
  "private": true,
  "license": "MPL 2.0",
  "bugs": {
    "url": "https://github.com/alexzedim/cmnw/issues",
    "discord": "alexzedim"
  },
  "author": {
    "name": "alexzedim",
    "email": "alexzedim@gmail.com",
    "url": "https://cmnw.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexzedim/cmnw.git"
  },
  "languageName": "node",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "rimraf dist && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/conglomerat/test/jest-e2e.json",
    "docker:build": "docker build -f docker/local.Dockerfile -t cmnw:%npm_package_version% -t cmnw:latest .",
    "version": "echo %npm_package_version%"
  },
  "dependencies": {
    "@alexzedim/blizzapi": "2.7.0",
    "@bull-board/express": "6.9.6",
    "@nestjs-modules/ioredis": "2.0.2",
    "@nestjs/axios": "4.0.0",
    "@nestjs/bullmq": "11.0.2",
    "@nestjs/common": "11.1.2",
    "@nestjs/core": "11.1.2",
    "@nestjs/mongoose": "11.0.3",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.2",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/typeorm": "11.0.0",
    "@types/async-csv": "2.1.4",
    "@types/cheerio": "1.0.0",
    "@types/fs-extra": "11.0.4",
    "async-csv": "2.1.3",
    "blizzapi": "2.6.5",
    "bull-board": "2.1.3",
    "bullmq": "5.53.0",
    "change-case": "^5.4.4",
    "cheerio": "1.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.4.0",
    "express": "5.1.0",
    "farmhash": "4.0.1",
    "fs-extra": "11.3.0",
    "http-proxy-agent": "7.0.2",
    "https-proxy-agent": "7.0.6",
    "ioredis": "5.6.1",
    "lodash": "4.17.21",
    "luxon": "3.6.1",
    "mongoose": "8.15.1",
    "ms": "2.1.3",
    "nestjs-bull-board": "1.2.1",
    "passport": "0.7.0",
    "passport-oauth2": "1.8.0",
    "pg": "^8.16.0",
    "playwright": "1.52.0",
    "redis": "5.1.1",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "snake-case": "4.0.0",
    "typeorm": "0.3.24",
    "typeorm-naming-strategies": "4.1.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.27.1",
    "@jest/globals": "30.0.0-beta.3",
    "@nestjs/cli": "11.0.7",
    "@nestjs/testing": "11.1.2",
    "@types/config": "3.3.5",
    "@types/express": "5.0.2",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.17",
    "@types/ms": "2.1.0",
    "@types/node": "22.15.24",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "eslint": "9.27.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "run-script-webpack-plugin": "0.2.3",
    "supertest": "7.1.1",
    "ts-jest": "29.3.4",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "webpack": "5.99.9",
    "webpack-node-externals": "3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/configuration": "<rootDir>/libs/configuration/src",
      "@app/configuration/(.*)": "<rootDir>/libs/configuration/src/$1",
      "@app/core": "<rootDir>/libs/core/src",
      "@app/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@app/mongo": "<rootDir>/libs/mongo/src",
      "@app/mongo/(.*)": "<rootDir>/libs/mongo/src/$1",
      "@app/pg": "<rootDir>/libs/pg/src",
      "@app/pg/(.*)": "<rootDir>/libs/pg/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1"
    }
  }
}